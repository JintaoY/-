<view class="content"><view class="row b-b"><text class="tit">经销商ID</text><text class="text">{{distributor.did}}</text></view><view class="row b-b"><text class="tit">手机号</text><text class="text">{{distributor.dphonenum}}</text></view><view class="row b-b"><text class="tit">姓名</text><text class="text">{{distributor.dname}}</text></view><view class="row b-b"><text class="tit">性别</text><text class="text">{{distributor.dsex}}</text></view><view class="row b-b"><text class="tit">地址</text><text class="text">{{distributor.daddress}}</text></view><view class="row b-b"><text class="tit">积分</text><text class="text">{{distributor.dintegral}}</text></view><view class="row b-b"><text class="tit">账号状态</text><block wx:if="{{distributor.dstatus==1}}"><text class="text">已开启</text></block><block wx:else><text class="text">已关闭</text></block></view><button data-event-opts="{{[['tap',[['toReturn',['$event']]]]]}}" class="add-btn" bindtap="__e">返回</button></view>